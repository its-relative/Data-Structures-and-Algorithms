    "def find_max(arr):\n",
    "    count = 0\n",
    "    for i in range(len(arr)):\n",
    "        if max(arr[i],count) == arr[i]:\n",
    "            count = arr[i]\n",
    "    return count\n",
    "new_arr = [10, 24, 8, 43, 35, 12, 67, 89, 45, 2]\n",
    "find_max(new_arr)"
   ]
