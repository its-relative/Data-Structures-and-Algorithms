    "def vow_counter(s):\n",
    "\n",
    "    vowels = [\"a\",\"e\",\"i\",\"o\",\"u\"]\n",
    "    counter = 0\n",
    "    for i in list(s.lower()):\n",
    "        if i in vowels:\n",
    "            counter += 1\n",
    "    return counter\n",
    "\n",
    "s = input()\n",
    "vow_counter(s)"
   ]
  }
